# ===========================================
# .env.example - Variables d'environnement
# Copier vers .env et remplir les valeurs
# ===========================================

# ========== API KEYS ==========
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
OPENSTREETMAP_API_KEY=your_osm_api_key_here

# ========== FLASK CONFIGURATION ==========
FLASK_SECRET_KEY=your_super_secret_key_for_production
FLASK_ENV=development
FLASK_DEBUG=True

# ========== DATABASE ==========
DATABASE_URL=sqlite:///mapaq_data.db
POSTGRES_URL=postgresql://user:password@localhost:5432/mapaq_db

# ========== INTEGRATION TRACK-A/C ==========
TRACK_A_API_URL=http://localhost:8000
TRACK_A_API_KEY=your_track_a_api_key
TRACK_C_API_URL=http://localhost:9000
TRACK_C_API_KEY=your_track_c_api_key

# ========== LOGGING ==========
LOG_LEVEL=INFO
LOG_FILE=logs/mapaq_track_b.log

# ========== CACHE & PERFORMANCE ==========
REDIS_URL=redis://localhost:6379/0
CACHE_TIMEOUT=3600

# ========== MONITORING ==========
PROMETHEUS_PORT=8080
GRAFANA_URL=http://localhost:3000
